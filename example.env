# Retry settings
MAX_RETRIES=3
RETRY_DELAY_SECONDS=5

# Development/Debug Settings
DEBUG=true
LOG_LEVEL=DEBUG
DAGSTER_DBT_PARSE_PROJECT_ON_LOAD=1